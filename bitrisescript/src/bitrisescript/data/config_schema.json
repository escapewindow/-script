{
    "title": "bitrisescript config schema",
    "type": "object",
    "required": [
        "bitrise",
        "taskcluster_scope_prefixes",
        "trust_domain"
    ],
    "properties": {
        "bitrise": {
            "type": "object",
            "required": ["access_token"],
            "properties": {
                "access_token": {
                    "type": "string"
                }
            }
        },
        "taskcluster_scope_prefixes": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "trust_domain": {
            "type": "string"
        }
    }
}
